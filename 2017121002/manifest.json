{
  "manifest_version": 2,
  "name": "Kittenator",
  "version": "0.1",
  "web_accessible_resources": ["kittens/*.jpg"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["kittens.js"]
    }
  ],
  "background": {
  "scripts": ["background.js"]
},
"browser_action": {
  "default_icon": "icon.png"
}
}
